# Cấu hình môi trường cho Thùng Rác Thông Minh

# Port để chạy server
PORT=3000

# URL kết nối MongoDB
# Local MongoDB
MONGODB_URI=mongodb://localhost:27017/thung-rac-thong-minh

# Hoặc MongoDB Atlas (cloud)
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/thung-rac-thong-minh

# Secret key cho JWT (thay đổi trong production)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Cấu hình bổ sung (tùy chọn)
NODE_ENV=development 